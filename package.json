  },
  "author": "Matt Dolan <mdolan@atlassian.com>",
  "homepage": "https://bitbucket.org/mgduk_atlassian/story-boards#readme",
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "lodash": "^4.17.4",
    "node-trello": "^1.2.1",
    "path": "^0.12.7",
    "snyk": "^1.319.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "snyk": true
}
